#include "main.h"
/**
* main - utility functionentry point
* Description: Natural Num. proj
* Return: 0 in success & 1 error
*/
int main(void)
{
  int i = 0;
  int sum = 0; 
  for (i = 0; i < 1024; i++)
    {
      if (i % 3 == 0 || i % 5 == 0)
	sum = sum + i;
      printf("%d\n", sum);
    }
  return (0);
}


